// divu masivu reiz
#include <iostream>

int main() {
    int arr1[10], arr2[10], multi = 0;
    
    std::cout << "Pirmais masivs:" << std::endl;
    for(int i = 0; i < 10; i++){
        std::cin >> arr1[i];
}
    std::cout << "Ortais masivs:" << std::endl;
    for(int i = 0; i < 10; i++){
    std::cin >> arr2[i];
}
    std::cout << "Reizinājumi"<< std::endl;
    for(int i = 0; i < 10; i++){
        multi = arr1[i] * arr2[i];
        std::cout << multi << std::endl;
}
        
    return 0;
}
