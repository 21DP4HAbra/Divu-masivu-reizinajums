#include <cmath>
#include <iostream>

double length(int x1, int x2, int y1, int y2) {
    return sqrt(pow((x2-x1),2)+pow((y2-y1),2));
}

int main() {
    int x1 = 0;
    int y1 = 0;
    int x2 = 0;
    int y2 = 0;
    
    std::cin >> x1;
    std::cin >> y1;
    std::cin >> x2;
    std::cin >> y2;
    
    std::cout << length(x1, x2, y1, y2) << std::endl;
    
    
    
    
    return 0;
